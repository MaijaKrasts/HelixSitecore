@model Sitecore.Mvc.Presentation.RenderingModel
@using Sitecore.Mvc

<footer>
    <div class="container">
            <div class="col-md-4">
                <!-- [Meta Navigation] -->
                <ul class="nav nav-stacked">
                    @Html.Sitecore().Field("Main item")
                </ul>
                <!-- [/Meta Navigation] -->
            </div>
        </div>
    </div>
    <div class="footer-bottom text-center">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <small class="copyright">
                        <span style="line-height: 17.1428px;">@Html.Sitecore().Field("Copyright")</span>
                    </small>
                    <div>
                        @Html.Sitecore().Field("Other links")
                    </div>
                </div>
                <div>
                </div>
            </div>
        </div>
    </div>
</footer>
